{% extends 'base.html.twig' %}

{% block body %}
    <h1>Objectif</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ objectif.id }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ objectif.annee }}</td>
            </tr>
            <tr>
                <th>Objectif</th>
                <td>{{ objectif.objectif }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ objectif.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ objectif.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ objectif.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ objectif.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('objectif_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('objectif_edit', { 'id': objectif.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
