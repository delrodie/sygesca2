{% extends 'base.html.twig' %}

{% block body %}
    <h1>District</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ district.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ district.nom }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ district.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ district.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ district.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ district.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ district.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_district_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_district_edit', { 'id': district.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
